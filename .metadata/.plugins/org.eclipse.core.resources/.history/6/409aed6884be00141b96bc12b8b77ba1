package Reflections;

public class Timers {

	public static void main(String[] args) {
		Book b = new Book();
		
	   long startTime = System.nanoTime();
	   for (int i = 0; i<=1000; i++)
	   {
		   b.ChangeYOP(1992);
	   }
	   long estimatedTime = System.nanoTime() - startTime;
	   System.out.println("1000 wywołań normalnych dało czas: "+estimatedTime);
	
	}

}
